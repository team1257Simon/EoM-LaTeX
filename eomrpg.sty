\newcounter{cnt}
\newcommand*{\dice}[1]{%
  \foreach \tpe / \num in {#1}%
  {%
    \forloop{cnt}{0}{\value{cnt} < \num}%
    {%
      \IfStrEqCase{\tpe}%
      {%
        {b}{\boost}
        {g}{\ability}
        {y}{\proficiency}
        {k}{\setback}
        {p}{\difficulty}
        {r}{\challenge}
        {w}{\magic}
      }%
    }%
  }%
}
\newcommand*{\results}[1]{%
  \foreach \tpe / \num in {#1}%
  {%
    \forloop{cnt}{0}{\value{cnt} < \num}%
    {%
      \IfStrEqCase{\tpe}%
      {%
        {s}{\success}
        {a}{\advantage}
        {r}{\triumph}
        {f}{\failure}
        {t}{\threat}
        {d}{\despair}
        {o}{\overload}
      }%
    }%
  }%
}
